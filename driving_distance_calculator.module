<?php

/**
 * @file
 * Module bootstrap for driving_distance_calculator.
 *
 * This module no longer depends on Webform. The standalone MovingQuoteForm
 * implements the behavior originally provided by the webform export.
 */

/**
 * Implements hook_help() - optional.
 */
function driving_distance_calculator_help($route_name, \\Drupal\Core\Routing\RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'help.page.driving_distance_calculator':
      return '<p>' . t('Provides a standalone moving quote form, pricing service and price endpoint.') . '</p>';
  }
}
